{
  "name": "nttp-typescript",
  "version": "1.0.0",
  "type": "module",
  "description": "Natural Text Transfer Protocol - TypeScript Implementation",
  "main": "dist/index.js",
  "bin": {
    "nttp": "./bin/nttp.js"
  },
  "scripts": {
    "setup": "npm run build && node bin/nttp.js setup",
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "doctor": "npm run build && node bin/nttp.js doctor",
    "test-db": "npm run build && node bin/nttp.js test-db",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "lint": "eslint src tests",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "postinstall": "npm run build 2>/dev/null || true"
  },
  "keywords": [
    "nttp",
    "natural-language",
    "sql",
    "database",
    "llm",
    "claude",
    "api"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.30.1",
    "@faker-js/faker": "^10.1.0",
    "@fastify/cors": "^9.0.1",
    "@fastify/swagger": "^8.14.0",
    "@fastify/swagger-ui": "^4.0.0",
    "better-sqlite3": "^9.6.0",
    "boxen": "^8.0.1",
    "chalk": "^4.1.2",
    "cli-table3": "^0.6.5",
    "dotenv": "^16.4.5",
    "fastify": "^4.26.2",
    "figlet": "^1.9.4",
    "gradient-string": "^3.0.0",
    "knex": "^3.1.0",
    "knex-schema-inspector": "^3.1.0",
    "mssql": "^11.0.1",
    "mysql2": "^3.11.5",
    "ora": "^5.4.1",
    "pg": "^8.13.1",
    "pino": "^9.0.0",
    "pino-pretty": "^11.0.0",
    "prompts": "^2.4.2",
    "update-notifier": "^7.3.1",
    "zod": "^3.23.8",
    "zod-to-json-schema": "^3.23.0"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.10",
    "@types/figlet": "^1.7.0",
    "@types/gradient-string": "^1.1.6",
    "@types/mssql": "^9.1.5",
    "@types/node": "^20.12.7",
    "@types/pg": "^8.11.10",
    "@types/prompts": "^2.4.9",
    "@typescript-eslint/eslint-plugin": "^7.7.0",
    "@typescript-eslint/parser": "^7.7.0",
    "@vitest/coverage-v8": "^1.5.0",
    "esbuild": "^0.20.2",
    "eslint": "^8.57.0",
    "prettier": "^3.2.5",
    "tsx": "^4.7.2",
    "typescript": "^5.4.5",
    "vitest": "^1.5.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
